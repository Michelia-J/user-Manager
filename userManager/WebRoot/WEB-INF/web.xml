<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  
  <servlet>
  <servlet-name>userLoginDo</servlet-name>
  <servlet-class>com.fz.servlet.loginDo</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>userLoginDo</servlet-name>
  <url-pattern>/login/loginDo</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>indexShopCar</servlet-name>
	<servlet-class>com.fz.servlet.indexShopCar</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>indexShopCar</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
     <servlet-name>delUser</servlet-name>
     <servlet-class>com.fz.servlet.delUser</servlet-class>
  </servlet>


  <servlet-mapping>
     <servlet-name>delUser</servlet-name>
     <url-pattern>/userManage/delUser</url-pattern>
  </servlet-mapping>

  
  <servlet-mapping>
     <servlet-name>recordAccount</servlet-name>
     <url-pattern>/shopCar/account</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>recordAccount</servlet-name>
     <servlet-class>com.fz.servlet.recordAccount</servlet-class>
  </servlet> 
  
<servlet>
     <servlet-name>docar</servlet-name>
     <servlet-class>com.fz.servlet.doCar</servlet-class>
  </servlet>


  <servlet-mapping>
     <servlet-name>docar</servlet-name>
     <url-pattern>/shopCar/docar</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>/login/login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
